{
  "name": "taskcluster-client",
  "version": "2.0.0",
  "author": "Jonas Finnemann Jensen <jopsen@gmail.com>",
  "description": "Client for interfacing taskcluster components",
  "license": "MPL-2.0",
  "files": [
    "build",
    "apis.json"
  ],
  "main": "build/index.js",
  "browser": "build/browser.js",
  "scripts": {
    "build:browser": "webpack",
    "build:node": "babel src --out-dir build --source-maps inline --ignore node_modules",
    "build": "npm run build:node && npm run build:browser",
    "docs": "bin/docs.js",
    "fetch": "bin/fetch.js",
    "list": "bin/list.js",
    "prepublish": "npm run test",
    "show": "bin/show.js",
    "test:node": "npm run build:node && mocha test/*_test.js",
    "test:browser": "npm run build:browser && mocha-phantomjs -R spec --bail test/browser_test.html",
    "test": "npm run test:node && npm run test:browser",
    "update": "npm run fetch && npm run build && npm run docs"
  },
  "repository": "taskcluster/taskcluster-client",
  "dependencies": {
    "amqplib": "0.4.2",
    "assert": "1.4.1",
    "debug": "2.2.0",
    "hawk": "5.1.2",
    "lodash.clonedeep": "4.5.0",
    "slugid": "1.1.0",
    "sockjs-client": "1.1.1",
    "superagent": "2.2.0",
    "url-join": "1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-taskcluster": "3.0.0",
    "clean-webpack-plugin": "0.1.10",
    "cliff": "0.1.10",
    "common-tags": "1.3.1",
    "got": "6.3.0",
    "json-loader": "0.5.4",
    "json-schema-to-markdown": "1.0.3",
    "json-stable-stringify": "1.0.1",
    "lodash": "4.15.0",
    "mocha": "2.2.1",
    "mocha-phantomjs": "3.5.3",
    "nock": "8.0.0",
    "phantomjs": "1.9.16",
    "taskcluster-base": "6.2.0",
    "webpack": "1.13.2",
    "webpack-node-externals": "1.3.3"
  }
}
